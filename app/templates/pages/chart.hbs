{{> html/start title="Chart" settings=settings }}
{{> html/header }}
<main>
  <div class="chart-page">
    <section class="filters">
      <h2 class="filters__title">Filters</h2>
      <ul class="filters__buttons">
        <li class="filters__buttons-item">Salary</li>
        <li class="filters__buttons-item">Work hours</li>
        <li class="filters__buttons-item">Road time</li>
        <li class="filters__buttons-item">Interesting</li>
        <li class="filters__buttons-item">Requires Education</li>
      </ul>
    </section>
    <section class="chart">
      <div class="chart__title-add-vacancy-block">
        <h1 class="chart__title">Chart</h1>
        <a href="/add-vacancy" class="chart__add-vacancy-link">
          <img src="/static-assets/img/plus.svg" alt="add" class="chart__add-vacancy-img">
          Add
        </a>
      </div>


      <div class="positions-block"></div>
    </section>
  </div>
</main>

{{> html/footer }}
<script type="text/javascript" src="static-assets/js/chart.js"></script>
{{> html/end }}
